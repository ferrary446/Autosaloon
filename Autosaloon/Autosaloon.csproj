    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="LoginScreen.Designer.cs~RFc41d1.TMP" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="RJCodeAdvance.RJControl2s" Version="1.0.0">
